    "@mdx-js/tag": "^0.20.3",
    "@westegg/gatsby-theme-core": "0.0.2-3",
    "gatsby-mdx": "^0.6.2",
    "gatsby": "^2.3.4",
    "prettier": "^1.17.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
