{
  "name": "westegg-core",
  "version": "0.0.2-3",
  "description": "Westegg Core Starter",
  "main": "gatsby-config.js",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "lint": "prettier --write --config package.json \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.15",
